            <string>///controls
var rkey = keyboard_check(vk_right);
var lkey = keyboard_check(vk_left);
var jkey = keyboard_check(vk_up);
// checking ground
if place_meeting(x, y+1, obj_wall) {
    vspd = 0;
    
//jump    
    if (jkey) {
        vspd = -jspd;
    }
} else {
//gravity
    if (vspd &lt; 10) {
        vspd += grav;
    }
}
//right
